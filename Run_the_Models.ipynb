    "def add_columns_to_data(dataframe):\n",
    "    dataframe['Duration (s)'] = pd.to_timedelta(dataframe['Duration'], errors='coerce').dt.total_seconds()\n",
    "    dataframe['velocity (m/s)'] = dataframe['Distance (m)'] / dataframe['Duration (s)']\n",
    "    dataframe['velocity (km/h)'] = 3.6 * dataframe['velocity (m/s)']\n",
    "    \n",
    "    elevation_minus_99_count = (dataframe['Elevation (m)'] == -99).sum()\n",
    "    if elevation_minus_99_count > 0:\n",
    "        print(f\"Warning: {elevation_minus_99_count} rows have an 'Elevation (m)' value of -99.\")\n",
    "        sys.exit() \n",
    "\n",
    "    dataframe['elevation_diff'] = dataframe['Elevation (m)'].diff().fillna(0)\n",
    "    \n",
    "    for row in dataframe.index:\n",
    "        # Check for a distance of 0.00 (handle division by zero)\n",
    "        if dataframe.loc[row, 'Distance (m)'] == 0.00:\n",
    "            dataframe.loc[row, 'slope (deg)'] = 0.00  # Assign 0.00 if distance is 0\n",
    "        else:\n",
    "            # Calculate the gradient (elevation difference / distance)\n",
    "            dataframe.loc[row, 'gradient'] = dataframe.loc[row, 'elevation_diff'] / dataframe.loc[row, 'Distance (m)']\n",
    "            dataframe.loc[row, 'slope (deg)'] = np.degrees(np.arctan(dataframe.loc[row, 'gradient']))\n",
    "    return dataframe"
   ]
