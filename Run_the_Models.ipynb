    "def diesel_used(GPS, mass, drag_coeff, area, r0, r1):\n",
    "    rho = 1.225  # air density (kg/m^3)\n",
    "    m = mass\n",
    "    m_rot = 0.03 * m  # rotational mass (3% of vehicle mass)\n",
    "    A = area  # vehicle frontal area (m^2)\n",
    "    g = 9.81  # gravitational acceleration (m/s^2)\n",
    "    diesel_dens = 0.85  # diesel density (kg/L)\n",
    "    r_0 = r0\n",
    "    r_1 = r1\n",
    "    Cd = drag_coeff  # drag coefficient\n",
    "\n",
    "    speed_unchanged = GPS['velocity (km/h)'].copy()\n",
    "    filename_string = 'Unchanged'\n",
    "    GPS, filename_string = speed_change_model(model, GPS, value_80_smo, value_90_smo, value_100_smo)\n",
    "\n",
    "    GPS['fatal'] = np.exp(0.08*(GPS['velocity (km/h)'] - speed_unchanged))\n",
    "    GPS['serious'] = np.exp(0.06*(GPS['velocity (km/h)'] - speed_unchanged))\n",
    "    GPS['minor'] = np.exp(0.04*(GPS['velocity (km/h)'] - speed_unchanged))\n",
    "    GPS['non-injury'] = np.exp(0.02*(GPS['velocity (km/h)'] - speed_unchanged))\n",
    "    \n",
    "    # Loop over each row of the DataFrame to calculate fuel economy and fuel used\n",
    "    for row in GPS.index:\n",
    "        v = GPS.loc[row, 'velocity (km/h)']  # velocity in km/h\n",
    "        v = np.nan_to_num(v, nan=0, posinf=0, neginf=0)\n",
    "        theta = GPS.loc[row, 'slope (deg)']  # slope in degrees\n",
    "    \n",
    "        # Handle zero velocity cases\n",
    "        if v == 0:\n",
    "            GPS.loc[row, 'Fuel_Economy (g/km)'] = 0  # No fuel consumed if velocity is zero\n",
    "            GPS.loc[row, 'Fuel_Economy (L/100km)'] = 0\n",
    "            GPS.loc[row, 'Fuel_used (L)'] = 0\n",
    "        else:\n",
    "            bea, be = class_equations(classy, v)\n",
    "            \n",
    "            # Calculate fuel economy in g/km\n",
    "            GPS.loc[row, 'Fuel_Economy (g/km)'] = 0.000278 * (\n",
    "                (rho * Cd * A / 2) * (v / 3.6) ** 2  # aerodynamic drag force\n",
    "                + m * g * (r_0 + r_1 * (v / 3.6))  # rolling resistance\n",
    "                + (m + m_rot) * bea  # engine efficiency\n",
    "            ) * be  # final fuel economy calculation\n",
    "            + m * g * np.sin(np.radians(theta))  # grade resistance\n",
    "    \n",
    "            # Convert fuel economy to L/100km\n",
    "            GPS.loc[row, 'Fuel_Economy (L/100km)'] = GPS.loc[row, 'Fuel_Economy (g/km)'] / (10 * diesel_dens)\n",
    "    \n",
    "            # Calculate the fuel used (in liters)\n",
    "            GPS.loc[row, 'Fuel_used (L)'] = (GPS.loc[row, 'Fuel_Economy (L/100km)'] * GPS.loc[row, 'Distance (m)']) / (100 * 1000)\n",
    "    \n",
    "    # Calculate total fuel used by the model\n",
    "    Fuel_used_model = GPS['Fuel_used (L)'].sum()\n",
    "    return GPS, Fuel_used_model, filename_string, bea, be"
   ]
